{
  "name": "cinemon",
  "script": "deno run -A -c tsconfig.json --unstable run.ts cinemon",
  "watch": [
    ".config",
    "cinemon",
    "documents",
    "messages",
    "queues",
    "rest-cinemon"
  ],
  "env": {
    "DEBUG": "*"
  },
  "env_prod": {
    "DEBUG": "*:cinemon:*",
    "NODE_ENV": "production"
  }
}
